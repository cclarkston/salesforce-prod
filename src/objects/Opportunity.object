				onSuccess:function(result){
					var resObj = result[0];
					if(resObj.success == &apos;false&apos;){
						//something wrong
						var error;
						if(resObj.errors instanceof Array){
							error = resObj.errors[0];
						}else{
							error = resObj.errors;
						}
