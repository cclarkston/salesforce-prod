    <fields>
        <fullName>AO4_Count__c</fullName>
        <description>In order to count the # of AO4s for the Center Dashboard reports, Preliminary Treatment Plan must be evaluated. Multi-select picklists must be evaluated one possible value at time, hence the nested IF statements.</description>
        <externalId>false</externalId>
        <formula>/*Old version of the formula - changed on 3/7/2013 by CM*/
/*IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Upper&quot;),
     IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Lower&quot;), 2, 
           IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Lower&quot;), 2, 
           1)),
IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Upper&quot;),
     IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Lower&quot;), 2, 
           IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Lower&quot;), 2, 
           1)), 
IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Lower&quot;),
     IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Upper&quot;), 2, 
           IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Upper&quot;), 2, 
           1)),
IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Lower&quot;),
     IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Upper&quot;), 2, 
           IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Upper&quot;), 2, 
           1)),
IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;DBL AO4&quot;), 
     2, 
           IF(INCLUDES( Preliminary_Treatment_Plan__c , &quot;DBL AO4 Edentulous&quot;), 
           2, 
     0
     )
    )
   )
  )
 )
)*/
/*new version of the formula - it&apos;s easier to manage and I can drive validation rules off this one to catch when people enter too many treatment options (CM 3/7/2013)*/
0 + if(includes(Preliminary_Treatment_Plan__c, &quot;AO4 Upper&quot;),1,0) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;AO4 Lower&quot;),1,0) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;AO4 Edentulous Upper&quot;),1,0) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;AO4 Edentulous Lower&quot;),1,0) 

+ if(includes(Preliminary_Treatment_Plan__c, &quot;DBL AO4 Edentulous&quot;),2,0) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;Transitional DBL AO4&quot;),2,if(includes(Preliminary_Treatment_Plan__c, &quot;DBL AO4&quot;),2,0)) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;Phased Barless Interim Upper AO4&quot;),1,0) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;Phased Barless Interim Lower AO4&quot;),1,0) 
+ if(includes(Preliminary_Treatment_Plan__c, &quot;Phased Barless Interim Double AO4&quot;),2,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>AO4 Count</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AO_4_Amount__c</fullName>
        <description>Displays the Negotiated Total Amount of treatments involving AO-4 procedures, for Center Sales Dashboard comparison.</description>
        <externalId>false</externalId>
        <formula>IF(  OR(INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Upper&quot;),
             INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Lower&quot;),
             INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Upper&quot;),
             INCLUDES( Preliminary_Treatment_Plan__c , &quot;AO4 Edentulous Lower&quot;),
             INCLUDES( Preliminary_Treatment_Plan__c , &quot;DBL AO4&quot;),
             INCLUDES( Preliminary_Treatment_Plan__c , &quot;DBL AO4 Edentulous&quot;)
            ), 
       Negotiated_Total_Amount__c, 
       0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>AO-4 Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
